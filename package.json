{
  "name": "precomp-frontend",
  "main": "node_modules/expo/AppEntry.js",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5001",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.5",
    "cookie-session": "^2.0.0-beta.3",
    "expo": "^30.0.1",
    "express": "^4.16.3",
    "mongodb": "^3.1.3",
    "react": "^16.3.2",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.3.2",
    "react-modal": "^3.4.4",
    "react-native": "https://github.com/expo/react-native/archive/sdk-30.0.0.tar.gz",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "^2.8.0",
    "reactstrap": "^6.0.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.1.1",
    "uuid": "^3.2.1",
    "webaudiofont": "^2.5.0",
    "webpack-dotenv-plugin": "^2.1.0"
  },
  "scripts": {
    "start": "node server.js",
    "start-server": "npm run build-server && node server.js",
    "start-dev": "react-scripts-ts start",
    "start-native": "concurrently \"npm run build-native -- -w\" \"expo start\"",
    "start-android": "expo start --android",
    "start-ios": "expo start --ios",
    "eject-native": "expo eject",
    "clean-native": "rimraf build-native",
    "build": "react-scripts-ts build",
    "build-native": "npm run clean-native && tsc -p src/native",
    "test": "node_modules/jest/bin/jest.js",
    "eject": "react-scripts-ts eject",
    "build-server": "node_modules/webpack/bin/webpack.js --config webpack.config.server.ts",
    "deploy-static": "REACT_APP_DEPLOY_BUILD=true npm run build && netlify deploy --prod",
    "deploy-api": "IS_HEROKU=true npm run build-server && git commit -am 'rebuilt server' && git push heroku master"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/body-parser": "^1.17.0",
    "@types/classnames": "^2.2.4",
    "@types/cookie-session": "^2.0.35",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.2",
    "@types/mongodb": "^3.1.4",
    "@types/node": "^10.3.3",
    "@types/react": "^16.3.18",
    "@types/react-bootstrap": "^0.32.10",
    "@types/react-dom": "^16.0.6",
    "@types/react-modal": "^3.1.2",
    "@types/react-native": "^0.57.2",
    "@types/react-router-dom": "^4.2.7",
    "@types/uuid": "^3.4.3",
    "concurrently": "^4.0.1",
    "jasmine": "^3.1.0",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.1",
    "wdio-jasmine-framework": "^0.3.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    }
  }
}
