{
    "compilerOptions": {
      "module":"es2015",
      "target": "es2015",
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
      "noImplicitAny": true,
      "sourceMap": true,
      "experimentalDecorators": true,
      "preserveConstEnums": true,
      "allowJs": true,
      "noUnusedLocals": true,
      "noUnusedParameters": false,
      "noImplicitReturns": false,
      "skipLibCheck": true,
      "moduleResolution": "node",
      "baseUrl": ".",
      "outDir": ".",
      "lib": ["es6", "dom"],
      "rootDir": "../../",
      "forceConsistentCasingInFileNames": false,
      "noImplicitThis": true,
      "strictNullChecks": true,
      "suppressImplicitAnyIndexErrors": true
    },
    "exclude": [
      "node_modules",
      "build",
      "scripts",
      "acceptance-tests",
      "webpack",
      "jest",
      "public",
      "src/native",
      "src/shared/test-data",
      "src/components",
      "src/registerServiceWorker.js",
      "src/index.tsx",
      "server.js",
      "migrate.js",
      "webpack.config.server.ts",
      "webpack.config.migrate.ts",
      "**/*.test.ts",
      "App.js"
    ]
  }